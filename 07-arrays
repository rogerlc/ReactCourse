//recorre arreglo solo con forEach
product.forEach(console.log);

//recorre arreglo con OF
for(const prod of product){
    console.log(prod);
}

// recorre arreglo de manera antigua
for (let i = 0; i < product.length; i++){
    const element = product[i];
    console.log(element);
}

console.log(product[0]);


